<?xml version="1.0" encoding="iso-8859-1"?>
<odoo>

    <act_window id="act_ring_template_bar_labels"
                name="Ring Barcode Labels" res_model="barcode.ring.lines"
                binding_model="product.template" view_mode="form" target="new"/>

    <act_window id="act_ring_barcode_labels"
                name="Ring Barcode Labels" res_model="barcode.ring.lines"
                binding_model="product.product" view_mode="form" target="new"/>

    <act_window id="act_ring_purchase_bar_labels"
                name="Ring Barcode Labels" res_model="barcode.ring.lines"
                binding_model="purchase.order" view_mode="form" target="new"/>

    <act_window id="act_ring_picking_bar_labels"
                name="Ring Barcode Labels" res_model="barcode.ring.lines"
                binding_model="stock.picking" view_mode="form" target="new"/>

    <menuitem id="menu_main_ring_barcode_labels"
              name="Ring Barcode Labels" parent="stock.menu_stock_root" sequence="5"
              groups="dynamic_barcode_ring_labels.group_ring_barcode_labels"/>

    <menuitem action="action_ring_barcode_labels"
              id="menu_action_ring_barcode_labels"
              parent="menu_main_ring_barcode_labels" sequence="10"
              groups="dynamic_barcode_ring_labels.group_ring_barcode_labels"/>

    <menuitem id="menu_ring_barcode_configuration"
              name="Barcode Config" action="action_ring_barcode_config"
              parent="menu_main_ring_barcode_labels" sequence="20"
              groups="dynamic_barcode_ring_labels.group_ring_barcode_labels"/>

</odoo>
